sh /etc/init.d/rc.mc_defaults

#Parameters here:
param set LED_RGB_MAXBRT 8


# Configure this as Quadrotor
# set MAV_TYPE 14

# Set mixer
set MIXER quad_tilt_H
set MIXER_AUX quad_tilt_H

set PWM_OUT 1234
